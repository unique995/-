public static String reverse(String str){//反转字符串
    char [] arr = str.toCharArray();
    char []array = new char[arr.length];
    for(int i = arr.length-1;i > 0;i--){
        for (int j = 0;j < array.length;j++){
            array[j] = arr[i];
            i--;
        }
    }
    String des = new String(array);
    return des;
}
