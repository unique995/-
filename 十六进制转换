import java.util.*;
import java.math.BigInteger;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        while(sc.hasNext()){
            String s = sc.nextLine();
            BigInteger b = new BigInteger(s.substring(2),16);
            System.out.println(b.toString(10));
        }
        sc.close();
    }
}
