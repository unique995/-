import java.util.*;
public class Main {
 public static void main(String []args){
   Scanner s=new Scanner(System.in);
   while(s.hasNext())
   {
	 int a=s.nextInt();
	 if(a<=0||a>=40000)
	 return ;
	 int m=a*a;
	 int sum=0;
	 int sum2=0;
	 while(a>0)
	 {
		 sum+=a%10;
		 a=a/10;
	 }
	 while(m>0)
	 {
		 sum2+=m%10;
		 m=m/10;
	 }
		
	 System.out.println(sum+" "+sum2);
   }
}

}
 
